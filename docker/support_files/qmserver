# Description: Service allowing connections to the OpenQM
# database server from telnet clients.
#
service qmserver
{
    protocol = tcp
    flags = REUSE
    socket_type = stream
    wait = no
    groups = yes
    user = root
    group = qmusers
    umask = 002
    server = /qmdata/qmsys/bin/qm
    server_args = -n
    log_on_failure += USERID
    disable = no
}
